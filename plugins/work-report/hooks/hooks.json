{
  "description": "工作报告插件 hooks：工作目录保护",
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/pre-bash.sh",
            "timeout": 5
          }
        ]
      }
    ]
  }
}
